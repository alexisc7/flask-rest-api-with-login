# Modo de ejecución: 'development' o 'production'
FLASK_ENV=development

# Clave secreta para firmar sesiones y cookies
SECRET_KEY=

# Ruta de la base de datos SQLite (puede ser relativa o absoluta)
DATABASE_URL=

# Clave secreta para firmar y verificar JWT
JWT_SECRET_KEY=

# Tiempo de expiración del token de acceso en segundos (opcional; default 3600)
JWT_ACCESS_TOKEN_EXPIRES=